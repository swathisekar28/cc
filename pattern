public class JavaApplication3 {

   
    public static void main(String[] args) {
        // TODO code application logic here
        int n=5;
        for(int i=1,p=1;i<=n;i++,p++)
        {
            for(int j=i;j<=n;j++)
            {
                System.out.print(p+"");
               
            }
            System.out.println();
        }
    }
    
}
