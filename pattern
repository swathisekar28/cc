4)package javaapplication3;

public class JavaApplication3 {

   
    public static void main(String[] args) {
        // TODO code application logic here
        int n=5,p=1;
        for(int i=1;i<n;i++,p++)
        {
            for(int j=i;j<=n;j++)
            {
                System.out.print("  ");
               
            }
            for(int j=1;j<=i;j++)
            {
                System.out.print(p+" ");
               
            }
            for(int j=1;j<i;j++)
            {
                System.out.print(p+" ");
               
            }
            System.out.println();
        }
        
        for(int i=1;i<=n;i++,p++)
        {
            for(int j=1;j<=i;j++)
            {
                System.out.print("  ");
               
            }
            for(int j=i;j<=n;j++)
            {
                System.out.print(p+" ");
               
            }
            for(int j=i;j<n;j++)
            {
                System.out.print(p+" ");
               
            }
            System.out.println();
        }
    }
    
}
