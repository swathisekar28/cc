package javaapplication3;

public class JavaApplication3 {

   
    public static void main(String[] args) {
        // TODO code application logic here
        int n=5;
        for(int i=1;i<=n;i++)
        {
            for(int j=i;j<=n;j++)
            {
                System.out.print(i+"");
            }
            System.out.println();
        }
    }
    
}
